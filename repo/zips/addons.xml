<?xml version='1.0' encoding='utf-8'?>
<addons><addon id="plugin.bilel.kodi" name="Bilel-KODI" version="0.0.3" provider-name="RIHANI Bilel">
    <requires>
        <import addon="xbmc.python" version="3.0.0" />
        <import addon="script.module.routing" version="0.2.0" />
        <import addon="script.module.simplejson" version="3.3.0" />
        <import addon="script.module.codequick" version="0.9.10" />
        <import addon="script.module.requests" version="2.9.1" />
    </requires>
    <extension point="xbmc.python.pluginsource" library="main.py">
        <provides>video</provides>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary lang="en_GB">video streaming</summary>
        <description lang="en_GB" />
        <language />
        <platform>all</platform>
        <license>MIT</license>
        <forum />
        <website />
        <email />
        <source />
        <news />
        <disclaimer />
        <assets>
            <icon>resources/icon.png</icon>
            <fanart>resources/fanart.jpg</fanart>
        </assets>
    </extension>
</addon><addon id="repository.bilel" name="Bilel Repository" version="0.0.2" provider-name="Bilel">
    <extension point="xbmc.addon.repository" name="Example Repository">
        <dir>
            <info compressed="false">https://raw.githubusercontent.com/rihanibilel/rihanibilel.github.io/master/repo/zips/addons.xml</info>
            <checksum>https://raw.githubusercontent.com/rihanibilel/rihanibilel.github.io/master/repo/zips/addons.xml.md5</checksum>
            <datadir zip="true">https://raw.githubusercontent.com/rihanibilel/rihanibilel.github.io/master/repo/zips/</datadir>
        </dir>
    </extension>
    <extension point="xbmc.addon.metadata">
        <summary>bilel for kodi</summary>
        <description>vide streaming</description>
        <disclaimer />
        <platform>all</platform>
        <assets>
            <icon>icon.png</icon>
            <fanart>fanart.jpg</fanart>
        </assets>
    </extension>
</addon></addons>